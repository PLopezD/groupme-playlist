@import 'yeticss'
header
  padding-top: 50px
.linkContainer
  border: 3px black solid
  margin-bottom: 3px
  height:75px
.link
  width:80%
  display:inline-block
  padding-bottom:50px

img 
  height: 60px
  width: 60px
  margin-right:5px
  margin-top:5px
  float:right
  border-radius:50%

@media screen and (max-width: 600px)
  img
    display:none
  .link
    width:100%
    padding-bottom:10px
  .title  
    padding-top:0px;